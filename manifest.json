{
    "manifest_version": 3,
    "name": "Show and Hide all calendars buttons for Google Calendarâ„¢",
    "version": "0.7.1",
    "description": "Quickly show, hide, or toggle all calendars in Google Calendar.",
    "icons": {
        "16": "images/icon-16.png",
